{
  "debugLog": {
    "on": true,
    "path": "./app.log"
  },
  "redis": {
    "host": "redis-beanq",
    "port": "6379",
    "password": "secret",
    "database": 0,
    "prefix": "test_queue",
    "maxRetries": 2,
    "poolSize": 30,
    "minIdleConnections": 10,
    "dialTimeout": "5s",
    "readTimeout": "3s",
    "writeTimeout": "3s",
    "poolTimeout": "4s",
    "maxLen": 1000
  },
  "health": {
    "port": "7779",
    "host": "0.0.0.0"
  },
  "broker": "redis",
  "consumerPoolSize": 100,
  "deadLetterIdle": "60s",
  "jobMaxRetries": 1,
  "keepFailedJobsInHistory": "3600s",
  "keepSuccessJobsInHistory": "3600s",
  "minConsumers": 10,
  "publishTimeOut": "10s",
  "consumeTimeOut": "10s",
  "history": {
    "on": false,
    "mongo": {
      "database": "lollipop_logs",
      "collections": {
        "event": "event_logs",
        "workflow": "workflow_logs",
        "manager": "managers",
        "opt": "opt_logs"
      },
      "username": "lollipop_logs",
      "password": "secret",
      "host": "mongo-beanq",
      "port": ":27017",
      "connectTimeout": "10s",
      "maxConnectionPoolSize": 200,
      "maxConnectionLifeTime": "600s"
    }
  }
}
